{
  "name": "flat-manager",
  "version": "1.0.0",
  "description": "Flat Management",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node --presets env",
    "release": "npm run clean && npm run build && npm run serve",
    "clean": "rm -rf dist && mkdir dist",
    "build": "babel . -s -D -d dist --presets env --ignore node_modules",
    "serve": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/icatalin201/flat-manager.git"
  },
  "keywords": [
    "nodejs",
    "flat",
    "management",
    "backend",
    "rest"
  ],
  "author": "Catalin Matache",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/icatalin201/flat-manager/issues"
  },
  "homepage": "https://github.com/icatalin201/flat-manager#readme",
  "dependencies": {
    "express": "^4.17.1",
    "pg": "^7.17.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "nodemon": "^2.0.2"
  }
}
